{
  "name": "copy-file-name",
  "displayName": "Copy File Name",
  "description": "Copy filename and change case and quickly print console.log",
  "publisher": "ouweiya",
  "version": "3.0.8",
  "icon": "icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/ouweiya/copy-file-name.git"
  },
  "engines": {
    "vscode": "^1.67.0"
  },
  "keywords": [
    "file name",
    "copy file name",
    "folder name",
    "copy folder name",
    "console",
    "console.log",
    "case",
    "change Case",
    "camelCase"
  ],
  "categories": [
    "Other"
  ],
  "galleryBanner": {
    "color": "#eff1f3",
    "theme": "light"
  },
  "main": "./extension.js",
  "activationEvents": [
    "onCommand:quickConsoleLog",
    "onCommand:copyFilename",
    "onCommand:camelCase",
    "onCommand:capitalCase",
    "onCommand:constantCase",
    "onCommand:dotCase",
    "onCommand:headerCase",
    "onCommand:noCase",
    "onCommand:paramCase",
    "onCommand:pascalCase",
    "onCommand:pathCase",
    "onCommand:sentenceCase",
    "onCommand:snakeCase"
  ],
  "contributes": {
    "commands": [
      {
        "command": "quickConsoleLog",
        "title": "Quick console.log"
      },
      {
        "command": "copyFilename",
        "title": "%copyFilename%"
      },
      {
        "command": "camelCase",
        "title": "camelCase (helloWorld)"
      },
      {
        "command": "pascalCase",
        "title": "pascalCase (HelloWorld)"
      },
      {
        "command": "snakeCase",
        "title": "snakeCase (hello_world)"
      },
      {
        "command": "capitalCase",
        "title": "capitalCase (Hello World)"
      },
      {
        "command": "constantCase",
        "title": "constantCase (HELLO_WORLD)"
      },
      {
        "command": "paramCase",
        "title": "paramCase (hello-world)"
      },
      {
        "command": "headerCase",
        "title": "headerCase (Hello-World)"
      },
      {
        "command": "noCase",
        "title": "noCase (hello world)"
      },
      {
        "command": "dotCase",
        "title": "dotCase (hello.world)"
      },
      {
        "command": "sentenceCase",
        "title": "sentenceCase (Hello world)"
      },
      {
        "command": "pathCase",
        "title": "pathCase (hello/world)"
      }
    ],
    "keybindings": [
      {
        "command": "quickConsoleLog",
        "key": "ctrl+shift+l",
        "when": "editorTextFocus"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "copyFilename",
          "group": "6_copypath@-10"
        }
      ],
      "editor/title/context": [
        {
          "command": "copyFilename",
          "group": "1_cutcopypaste@-10"
        }
      ],
      "editor/context": [
        {
          "group": "1_modification@-10",
          "submenu": "change-case",
          "when": "editorTextFocus"
        }
      ],
      "change-case": [
        {
          "command": "camelCase",
          "group": "change-case1@1"
        },
        {
          "command": "pascalCase",
          "group": "change-case1@2"
        },
        {
          "command": "snakeCase",
          "group": "change-case2@1"
        },
        {
          "command": "constantCase",
          "group": "change-case2@2"
        },
        {
          "command": "paramCase",
          "group": "change-case3@1"
        },
        {
          "command": "headerCase",
          "group": "change-case3@2"
        },
        {
          "command": "noCase",
          "group": "change-case4@1"
        },
        {
          "command": "capitalCase",
          "group": "change-case4@2"
        },
        {
          "command": "dotCase",
          "group": "change-case5@1"
        },
        {
          "command": "pathCase",
          "group": "change-case5@2"
        },
        {
          "command": "sentenceCase",
          "group": "change-case5@3"
        }
      ]
    },
    "submenus": [
      {
        "id": "change-case",
        "label": "Change Case"
      }
    ],
    "configuration": {
      "title": "Quick console.log",
      "properties": {
        "quickConsoleLog.semicolon": {
          "type": "boolean",
          "default": true,
          "description": "%quickConsoleLogDescription%"
        }
      }
    }
  },
  "scripts": {
    "package": "vsce package",
    "publish": "vsce publish patch"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "14.x",
    "@types/vscode": "^1.67.0",
    "@vscode/test-electron": "^2.1.3",
    "eslint": "^8.14.0",
    "glob": "^8.0.1",
    "mocha": "^9.2.2",
    "typescript": "^4.6.4"
  },
  "dependencies": {
    "change-case": "^4.1.2"
  }
}
