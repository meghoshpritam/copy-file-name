{
  "name": "copy-file-name-only",
  "displayName": "Copy File Name Only",
  "description": "Copy only the file name without the extension or the full path.",
  "publisher": "meghoshpritam",
  "version": "1.0.6",
  "icon": "icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/meghoshpritam/copy-file-name"
  },
  "engines": {
    "vscode": "^1.67.0"
  },
  "keywords": [
    "file name",
    "copy file name",
    "folder name",
    "copy folder name",
    "copy file name only",
    "copy folder name only",
    "copy file name without extension",
    "copy folder name without extension"
  ],
  "categories": [
    "Other"
  ],
  "galleryBanner": {
    "color": "#eff1f3",
    "theme": "light"
  },
  "main": "./extension.js",
  "activationEvents": [
    "onCommand:copyFilename",
    "onCommand:copyFilenameWithoutExtension"
  ],
  "contributes": {
    "commands": [
      {
        "command": "copyFilename",
        "title": "Copy File Name"
      },
      {
        "command": "copyFilenameWithoutExtension",
        "title": "Copy File Name Without Extension"
      }
    ],
    "keybindings": [],
    "menus": {
      "explorer/context": [
        {
          "command": "copyFilename",
          "group": "6_copypath@-10"
        },
        {
          "command": "copyFilenameWithoutExtension",
          "group": "6_copypath@-10"
        }
      ],
      "editor/title/context": [
        {
          "command": "copyFilename",
          "group": "1_cutcopypaste@-10",
          "when": "isFileSystemResource"
        },
        {
          "command": "copyFilenameWithoutExtension",
          "group": "1_cutcopypaste@-10",
          "when": "isFileSystemResource"
        }
      ]
    }
  },
  "scripts": {
    "package": "vsce package",
    "publish": "vsce publish patch"
  },
  "devDependencies": {
    "@types/vscode": "^1.67.0",
    "@vscode/test-electron": "^2.1.3",
    "@vscode/vsce": "^3.2.1",
    "eslint": "^8.57.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.3",
    "prettier": "^3.4.2",
    "typescript": "^4.6.4"
  }
}
