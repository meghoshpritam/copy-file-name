{
  "name": "quick-console-log",
  "displayName": "quick console.log",
  "description": "Quickly print console.log",
  "version": "0.0.1",
  "icon": "icon.png",

  "engines": {
    "vscode": "^1.5.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:quick-console-log"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "quick-console-log",
        "title": "Quick console log"
      }
    ],
    "keybindings": [
      {
        "command": "quick-console-log",
        "key": "shift+ctrl+l",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.67.0",
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "14.x",
    "eslint": "^8.14.0",
    "glob": "^8.0.1",
    "mocha": "^9.2.2",
    "typescript": "^4.6.4",
    "@vscode/test-electron": "^2.1.3"
  }
}
