{
  "name": "copy-file-name",
  "displayName": "Copy File Name",
  "description": "Quickly copy file names and console-utils",
  "publisher": "ouweiya",
  "version": "3.0.3",
  "icon": "icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/ouweiya/copy-file-name.git"
  },
  "engines": {
    "vscode": "^1.67.0"
  },
  "keywords": [
    "copy file name",
    "copy-file-name",
    "folder name",
    "copy folder name",
    "console"
  ],
  "categories": [
    "Other"
  ],
  "galleryBanner": {
    "color": "#232323",
    "theme": "dark"
  },
  "activationEvents": [
    "onCommand:quick-console-log",
    "onCommand:copy-filename"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "quick-console-log",
        "title": "Quick console.log"
      },
      {
        "command": "copy-filename",
        "title": "%copy-filename%"
      }
    ],
    "keybindings": [
      {
        "command": "quick-console-log",
        "key": "ctrl+shift+l",
        "when": "editorTextFocus"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "copy-filename",
          "group": "6_copypath@-10"
        }
      ],
      "editor/title/context": [
        {
          "command": "copy-filename",
          "group": "1_cutcopypaste@-10"
        }
      ]
    }
  },
  "scripts": {
    "package": "vsce package",
    "publish": "vsce publish"
  },
  "devDependencies": {
    "@types/vscode": "^1.67.0",
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "14.x",
    "eslint": "^8.14.0",
    "glob": "^8.0.1",
    "mocha": "^9.2.2",
    "typescript": "^4.6.4",
    "@vscode/test-electron": "^2.1.3"
  }
}
